<div class="card-content table-responsive">
  <table class="table table-hover">
    <thead class="text-primary">
      <tr>
        <th>Sr no.</th>
        <th>Name</th>
        <th>Email</th>
        <th>Phone No.</th>
        <th>Address</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% for( let i = 0; i < user.length; i++ ) { %>
      <tr>
        <td><%=i+1 %></td>
        <td><%= user[i].name %></td>

        <td><%= user[i].email %></td>
        <td><%= user[i].PhoneNo %></td>
        <td><%= user[i].Address %></td>
        <td>
          <a href="/api/milkmandata/delete?id=<%= user[i]._id %>" class="delete"
            ><i class="fas fa-trash-alt"></i
          ></a>
        </td>
      </tr>
      <% } %>
    </tbody>
  </table>
</div>
