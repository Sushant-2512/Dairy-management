<%- include('layouts/DashHeader') %>

    <body>




        <div class="wrapper">


            <div class="body-overlay"></div>


            <!-- Sidebar  -->
            <nav id="sidebar">

                <ul class="list-unstyled components">
                    <li class="active">
                        <a class="dashboard" href="/Dash/milkman"><i class="material-icons">dashboard</i><span>
                                Dashboard</span></a>
                    </li>

                    <div class="small-screen navbar-display">

                        <li class="">
                            <a href="/Dash/milkman/snffatratedata">
                                <i class="material-icons">library_books</i><span>SNF/Fat Rate</span></a>
                        </li>
                        <li class="">
                            <a href="mailto: abc@example.com">
                                <i class="material-icons">library_books</i><span>Report/Feedback</span></a>
                        </li>
                        <li class="d-lg-none d-md-block d-xl-none d-sm-block">
                            <a href="/logout"><i class="material-icons">person</i><span>Sign Out</span></a>
                        </li>
                    </div>

                </ul>


            </nav>



            <!-- Page Content  -->
            <div id="content">

                <div class="top-navbar">
                    <nav class="navbar navbar-expand-lg">
                        <div class="container-fluid">

                            <button type="button" id="sidebarCollapse" class="d-xl-block d-lg-block d-md-mone d-none">
                                <span class="material-icons">arrow_back_ios</span>
                            </button>

                            <p class="navbar-brand"><%= name %></p>

                            <button class="d-inline-block d-lg-none ml-auto more-button" type="button"
                                data-toggle="collapse" data-target="#navbarSupportedContent"
                                aria-controls="navbarSupportedContent" aria-expanded="false"
                                aria-label="Toggle navigation">
                                <span class="material-icons">more_vert</span>
                            </button>

                            <div class="collapse navbar-collapse d-lg-block d-xl-block d-sm-none d-md-none d-none"
                                id="navbarSupportedContent">
                                <ul class="nav navbar-nav ml-auto">

                                    <li class="nav-item">
                                        <a href="/logout"><i class="material-icons">person</i></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </nav>
                </div>


                <div class="main-content">

                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <div class="card card-stats">
                                <div class="card-header">
                                    <div class="icon icon-warning">
                                        <span class="material-icons">equalizer</span>
                                    </div>
                                </div>
                                <div class="card-content">
                                    <h3 class="card-title"><%= quantity %> </h3>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        MilkQuantity(10 days)
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-6 col-md-6 col-sm-6">
                            <div class="card card-stats">
                                <div class="card-header">
                                    <div class="icon icon-success">
                                        <span class="material-icons">
                                            attach_money
                                        </span>

                                    </div>
                                </div>
                                <div class="card-content">
                                    <h3 class="card-title"><%= total %> </h3>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        Income(10 days)
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class=" row row-table table ">

                            <div class="card" style="min-height: 485px">
                                <div
                                    class="card-header card-header-text d-flex justify-content-between align-items-center">
                                    <div>

                                        <h4 class="card-title">Recent Activities</h4>
                                        <p class="category">Transaction history</p>
                                    </div>

                                    <form class="form-inline" action="/api/milkman/search" method="POST">
                
                                        <div class="form-group mx-sm-3 mb-2">
                                          <input type="date" name="sdate" id="startingdate"class="form-control"  >
                                        </div>
                                        <div class="form-group mx-sm-3 mb-2">
                                            <input type="date" name="edate" class="form-control" id="endingdate" >
                                          </div>
                                        <button type="submit" class="btn btn-primary mb-2">Search</button>
                                      </form>
                                      <a href="http://localhost:3000/pdf/milkman.pdf" target="_blank"><button type="button" class="download "><i
                                        class="material-icons">arrow_downwards</i></button></a>
                                </div>
                                <%- include('layouts/milkmantable') %>

                            </div>


                        </div>



                        <footer class="footer p-0">
                            <div class="container-fluid d-flex justify-content-center">

                                <div class="col-md-6 d-flex justify-content-center">
                                    <div class="text-center d-flex align-items-center">

                                        <a href="mailto: abc@example.com">Contact Admin</a>
                                    </div>
                                    <p class="copyright "> &copy Fresh Fields
                                    </p>
                                </div>
                            </div>
                        </footer>

                    </div>



                </div>
            </div>



        </div>



        <%-include("layouts/homefooter") %>

        



            <script type="text/javascript">
                $(document).ready(function () {
                    $('#sidebarCollapse').on('click', function () {
                        $('#sidebar').toggleClass('active');
                        $('#content').toggleClass('active');
                    });

                    $('.more-button,.body-overlay').on('click', function () {
                        $('#sidebar,.body-overlay').toggleClass('show-nav');
                    });

                });





            </script>





    </body>

    </html>